package com.label.timesquid.bill {		import flash.display.MovieClip;		import com.label.timesquid.*;		import com.label.arduino.sensor.SensorEvent;		public class Hand extends MovieClip implements ILimb {				// Constants:		// Public Properties:		// Private Properties:		var min = 170;		var max = 290;		// UI Elements:				public function Hand()		{			//this.rotation = 120;			this.stop();			this.scaleY = 1;		}				public function sensorEvent(e:SensorEvent):void		{			trace('SENS');			setValue(e.val);		}				public function setValue(val:int) 		{						//Calc range									var newVal = (val/5);			trace("hand val", val, newVal);			//trace("range", range, "new Val", newVal, "calc: ", (newVal/10), val);			this.gotoAndStop(Math.floor(20-newVal));			//trace("this x: ", this.scaleX, this.scaleY);							}				public function isClosed():Boolean		{			return this.currentFrame>=15;					}	}	}