package com.label.timesquid{	import flash.display.MovieClip;	import flash.text.TextField;		import com.label.arduino.sensor.SensorEvent;	import flash.events.MouseEvent;	import com.label.arduino.sensor.FlexSensor;	import flash.events.TextEvent;	import flash.events.Event;		public class Config extends MovieClip	{		public var okButton:MovieClip;						public var serialVal:TextField;						public function Config()		{			trace("New Config");			okButton.buttonText.text = "OK";			okButton.addEventListener(MouseEvent.CLICK, closeEvent);		}								protected function closeEvent(e:MouseEvent):void		{			trace("CLOSE ddd");			dispatchEvent(new Event(Event.CLOSE, true, true));		}					}}