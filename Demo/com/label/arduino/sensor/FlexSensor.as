package com.label.arduino.sensor{		import flash.events.*;	public class FlexSensor  extends EventDispatcher implements IAnalogSensor	{				private var _max:int =  315;		private var _min:int = 125;		private var _diff:int = _max-_min;		private var steps:Number = 100/_diff;				public function setValue(val:int):void		{			//Convert val to scale of 100			var offsetVal = val;			if (offsetVal<_min)			{				offsetVal = _min;			}			if (offsetVal > _max)			{				offsetVal = _max;			}			offsetVal -=_min;						var calcVal = (offsetVal * steps);			dispatchEvent(new SensorEvent(SensorEvent.NEW_VALUE, true, true, calcVal, val));			//trace("Sensor val", val, calcVal, offsetVal, (offsetVal * steps));					}				public function setCalibration(min:int, max:int):void		{							_max =  max;			_min = min;			_diff = _max-_min;			steps = 100/_diff;			trace("CALIBRATION: ", _min, _max);		}				public function get min():int 		{			return _min;		}				public function get max():int 		{			return _max;		}	}	}