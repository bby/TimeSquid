package com.label.arduino.sensor{		import flash.events.*;	public class FlexSensor  extends EventDispatcher implements IAnalogSensor	{				private var _max:int =  315;		private var _min:int = 125;		private var _diff:int = _max-_min;		private var steps:Number = 100/_diff;				public function setValue(val:int):void		{			//Convert val to scale of 100			var offsetVal = val;			if (offsetVal<_min)			{				offsetVal = _min;			}			if (offsetVal > _max)			{				offsetVal = _max;			}			offsetVal -=_min;						var calcVal = (offsetVal * steps);			dispatchEvent(new SensorEvent(SensorEvent.NEW_VALUE, true, true, calcVal));			trace("Sensor val", val, calcVal, offsetVal, (offsetVal * steps));					}	}	}