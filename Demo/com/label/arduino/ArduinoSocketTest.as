package com.label.arduino{	import net.eriksjodin.arduino.*;	import net.eriksjodin.arduino.events.*;		public class ArduinoSocketTest extends Arduino	{		var val:Number = 0;		var speed:Number = 0.01;				public function ArduinoSocketTest(host:String = "127.0.0.1", port:int = 5331) 		{			_port = port;			trace("New ArduinoSocketTest");		}				public function createEvents():void		{			var pin:int = 0;			val+=speed;			//get sin val			var angle =2*Math.PI*val/10;						var value = Math.sin(angle);			for (pin = 0; pin<9; pin++)			{								dispatchEvent(new ArduinoEvent(ArduinoEvent.ANALOG_DATA, pin, (value*100)+100, _port));											}					}	}}